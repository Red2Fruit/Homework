pragma solidity ^0.8.19;

contract Test {
  address[] public whitelist;

    function addPush(address add) public {
           whitelist.push(add); 
        
        
    }

    function check(address add) public view returns (bool) {
         for(uint i = 0; i < whitelist.length; i++){
            if(whitelist[i] == add)
            return true;
         }
        return false;
    }
}
